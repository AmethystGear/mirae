Box flying_wall
{
	Box quotes 
	{
		string[] entrance  "STAY AWAY OR I WILL SHOOT!" "BUILD THE WALL! CLEAR! FLYING!" "YOU CANT FLY OVER ME BIGLY!" 
		string[] attack  "AIRMIGRATION SHALL BE DEFEATED" "This wall is so good. SO good. trust me. bigly" 
		string[] player_run  "good. Stay away. We dont want any bad hombres or jayanth knights in trump land"
		string[] player_victory  "but it was clear and everything" "honour shall not win!" "I am too bigly. I win. fake news!" "FAKE NEWS!"
		string[] mob_victory  "TRUMP TRUMP TRUMP" "Like all walls, I fly" "You are too slow!"
	}
	Box stats
	{
		int health 1000
		int speed 1
